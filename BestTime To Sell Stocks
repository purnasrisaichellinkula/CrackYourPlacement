class Solution {
public:
    int maxProfit(vector<int>& prices) {
        int n=prices.size();
        int min_price=prices[0];
        int ans=0;
        for(int i=1;i<n;i++)
        {
            int pro=prices[i]-min_price;
            ans=max(ans,pro);
            min_price=min(min_price,prices[i]);
        }
        return ans;
        
    }
};
